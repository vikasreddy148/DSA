import java.util.*;
import java.lang.*;
import java.io.*;

class BinaryToDecimal
{
	public static void main (String[] args) 
	{
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int binary = 0;
		int pow =0;
		while(n>0){
		   int r = n%2;
		  
		   binary =binary+ r*(int)Math.pow(10,pow);
		   pow++;
		    n=n/2;
		}
		System.out.println(binary);

	}
}
